first_number = input("enter first number : ")
secend_number = input("enter secend number : ")
operator = input("enter operator (= ,- , * , / , % ) :")

first_number = int(first_number)
secend_number = int(secend_number)

if operator == '+':
    print(first_number + secend_number)

elif operator == '-': 
    print(first_number - secend_number)    

elif operator == '*':
    print(first_number * secend_number)


elif operator == '/':
    print(first_number / secend_number)

elif operator == '%':
    print(first_number % secend_number) 

else :
    print("invalid operators")
 
#sucesfully run 